# 爬虫配置
TARGET_URL=https://www.zhipin.com/web/geek/job
RATE_LIMIT=3000
MAX_RETRIES=3
PAGE_TIMEOUT=30000
HEADLESS=false
USER_DATA_DIR=./user-data
BROWSER_PATH=

# 七牛云配置
QINIU_ACCESS_KEY=your_access_key
QINIU_SECRET_KEY=your_secret_key
QINIU_BUCKET=your_bucket_name
